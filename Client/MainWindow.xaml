<Window x:Class="Client.MainWindow"  
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"  
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
        xmlns:views="clr-namespace:Client.Views"  
        xmlns:local="clr-namespace:Client"  
        mc:Ignorable="d"  
        Title="Main Window" Height="600" Width="1000">  
    <TabControl>  
        <TabItem Header="Case Intake">  
            <views:CaseIntakeView />  
        </TabItem>  
        <TabItem Header="Scoping">  
            <views:ScopingView />  
        </TabItem>  
        <TabItem Header="Log Collector &amp; Viewer">
            <views:LogCollectorView />
        </TabItem>  
        <TabItem Header="Case Analysis">  
            <views:AnalysisView />  
        </TabItem>  
        <TabItem Header="Case Closing">  
            <views:ClosingView />  
        </TabItem>  
    </TabControl>  
</Window>
